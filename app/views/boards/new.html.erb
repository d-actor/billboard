<h2 class="center white-text">New Chart</h2>
<hr>
<div class="container white">
  <%= form_for @board do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <div class="center blue-text">
      <%= f.submit %>
    </div>
  <% end %>
</div>
<hr>
<h4 class="center"><%= button_to 'Back', boards_path, method: :get %></h4>
